{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@libs/prisma": "<rootDir>/../libs/prisma/src",
    "libs/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "libs/redis": "<rootDir>/../libs/redis/src",
    "@libs/meili/(.*)": "<rootDir>/../libs/meili/src/$1",
    "@libs/meili": "<rootDir>/../libs/meili/src"
  }
}